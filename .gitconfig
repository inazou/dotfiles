[alias]
		# いい感じのグラフでログを表示(ファイルの変更付き)
		graph = log --graph --date=short --decorate=short --stat --date-order --branches --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
		# 上の省略形(コミットのみ)
 		gr = log --graph --date=short --decorate=short --date-order --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
		st = status
		cm = commit
		df = diff
    # Untracked filesを表示せず，not stagedと，stagedだけの状態を出力する
    stt = status -uno
    # 行ごとの差分じゃなくて，単語レベルでの差分を色付きで表示する
		dff = diff --word-diff
		# gitのカレントブランチをpushできる
		p = !git push origin `git rev-parse --abbrev-ref HEAD`
